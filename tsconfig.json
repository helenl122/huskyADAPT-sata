{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "nativewind-env.d.ts",
    "components/*.jsx",
    "app/(games)/BlockGame.jsx",
    "app/*/.tsx",
    "components/game_components/SinglePress.tsx"
  ]
}
